{
  "configs": [
    {
      "block_type": "http",
      "server_http": {
        "listen_on": 80,
        "redirect": true
      },
      "server_https": {
        "listen_on": 443,
        "ssl_certificate_path": "",
        "ssl_certificate_key_path": ""
      },

      "clusters": [
        {
          "id": "backend",
          "server_configs": [
            {
              "type": "",
              "is_backup": true,
              "is_active": false,
              "base_url": "localhost:2222/api/v1",
              "health_check": {
                "is_disabled": false,
                "url": "localhost:2222/api/v1/health",
                "timeout": 3000,
                "tries": 3
              }
            }
          ]
        }
      ],
      "locations": [
        {
          "match_sub_uri": "/api/v1/",
          "stages": [
            {
              "action": "rewrite",
              "grouping_regex": "/sdfsdfds/sdfdsfds",
              "replacement_regex": "sdfsdfsdfdsf",
              "should_redirect": false
            },

            {
              "action": "proxy_pass",
              "url": "http://backend"
            }
          ]
        }
      ]
    }
  ]
}
